import model.game.SectorElement;

public class UnitTypeInCreationHandler extends PropertyHandler {
	public void setValue(String newValue, String oldValue,
			SectorElement sectorElement) {
		((Building) sectorElement).setUnitTypeInCreation(newValue);
	}

}
